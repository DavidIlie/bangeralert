{
   "pipeline": {
      "build": {
         "dependsOn": ["^build"],
         "outputs": [".next/**", ".expo/**"],
         "env": [
            "DATABASE_URL",
            "AUTH_SPOTIFY_CLIENT_ID",
            "AUTH_SPOTIFY_CLIENT_SECRET",
            "NEXTAUTH_URL",
            "NEXTAUTH_SECRET",
            "NEXTAUTH_EXPO_URL"
         ]
      },
      "lint": {
         "outputs": []
      },
      "dev": {
         "cache": false
      },
      "postinstall": {}
   }
}
