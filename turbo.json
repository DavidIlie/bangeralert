{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "db:generate": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "db:push": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "dev": {
      "dependsOn": ["^db:generate"],
      "cache": false
    },
    "patch": {
      "cache": false
    },
    "build": {
      "dependsOn": ["patch", "^build", "^db:generate"],
      "outputs": [".next/**", ".expo/**"]
    },
    "start": {
      "dependsOn": ["^build", "^db:generate"]
    },
    "lint": {
      "outputs": []
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "type-check": {
      "dependsOn": ["^db:generate"],
      "cache": false
    }
  },
  "globalEnv": [
    "DATABASE_URL",
    "NEXTAUTH_SECRET",
    "NEXTAUTH_URL",
    "NEXTAUTH_EXPO_URL",
    "OPENAI_APIKEY"
  ]
}
